oSkill(JO,YJ)    endif    if Jp!=null and Jp!=Yj then        call SelectHeroSkill(Jp,YJ)    endifendfunctionfunction Yk takes integer N7,unit qf,unit qF returns nothing    local integer T8=1    local integer U8=GetUnitAbilityLevel(qf,N7)    loop        exitwhen T8>U8        call SelectHeroSkill(qF,N7)        set T8=T8+1    endloopendfunctionfunction YK takes unit YE,unit Yl returns nothing    call AddHeroXP(YE,GetHeroXP(Yl),false)    call SvInt(GetHandleId(YE),"XPGiven",GetHeroXP(Yl))    call SvInt(GetHandleId(YE),"XPEarned",0)    call Yk('A0N8',Yl,YE)    call Yk('A0NB',Yl,YE)    call Yk('A0N7',Yl,YE)    call Yk('Aamk',Yl,YE)    call Yk('A0MW',Yl,YE)endfunctionfunction Kjv takes nothing returns nothing    local unit v1v=GetTriggerUnit()    local player j8=GetOwningPlayer(v1v)    local integer vYv=GetHandleId(j8)    local unit YE    call YI(v1v,'A0MW')    set YE=CreateUnit(GetOwningPlayer(v1v),'H00J',GetUnitX(v1v),GetUnitY(v1v),0)    call YK(YE,v1v)    call Ye(YE,v1v)    call SvUnit(vYv,"Kobold"+I2S(GetUnitAbilityLevel(v1v,'A0MW')),YE)    if(GetUnitAbilityLevel(v1v,'A0MW')>0)then        set ie[(16*GetUnitAbilityLevel(v1v,'A0MW'))+GetPlayerId(j8)]=YE    endifendfunctionfunction Ym takes unit Jo,unit JO,integer YJ,integer Yg,integer YG returns nothing    if Yg>YG then        call SelectHeroSkill(JO,YJ)    elseif YG>Yg then        call SelectHeroSkill(Jo,YJ)    endifendfunctionfunction YM takes unit Jo,unit JO returns nothing    local integer y8=GetHeroXP(Jo)    local integer y9=GetHeroXP(JO)    local integer yW=GetHandleId(Jo)    local integer yx=GetHandleId(JO)    local integer yZ=LdInt(yW,"XPGiven")    local integer y4=LdInt(yx,"XPGiven")    local integer VX    if y8>y9 then        set VX=y8-y9        call AddHeroXP(JO,VX,true)        if y9!=GetHeroXP(JO)then            call SvInt(yx,"XPGiven",y4+VX)        endif    elseif y8<y9 then        set VX=y9-y8        call AddHeroXP(Jo,VX,true)        if y8!=GetH